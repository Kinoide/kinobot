  "version": "0.2.0",
  "configurations": [
    {
      "name": "deno kinobot",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--inspect-brk",
        "--allow-read",
        "--allow-net",
        "--allow-env",
        "-A",
        "src/start.ts"
      ],
      "attachSimplePort": 9229
    }
  ]
}
